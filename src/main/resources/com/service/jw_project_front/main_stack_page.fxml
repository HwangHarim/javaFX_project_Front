<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>

<StackPane
        fx:id="stackPane"
        xmlns="http://javafx.com/javafx/17.0.2-ea"
        xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.service.jw_project_front.controller.HomeStackController">
    <GridPane alignment="CENTER" hgap="10" vgap="10">
        <Button onMouseClicked="#moveAutoCookPage" prefHeight="180.0" prefWidth="280.0"
                text="자동 요리"
                GridPane.columnIndex="0" GridPane.rowIndex="0"/>
        <Button onMouseClicked="#moveSelfCookPage" prefHeight="180.0" prefWidth="280.0"
                text="수동 요리"
                GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <Button onMouseClicked="#moveLogCookPage" prefHeight="180.0" prefWidth="280.0"
                text="최근 요리"
                GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <Button onMouseClicked="#moveCleanOvenPage" prefHeight="180.0" prefWidth="280.0"
                text="오븐 세척" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <columnConstraints>
            <ColumnConstraints/>
            <ColumnConstraints/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints/>
            <RowConstraints/>
        </rowConstraints>
    </GridPane>
</StackPane>